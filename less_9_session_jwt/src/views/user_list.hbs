<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Login</th>
      <th scope="col">Email</th>
      <th scope="col">Photo</th>
    </tr>
  </thead>
  <tbody class="table-striped">
    {{#each users}}
      <tr>
        <th scope="row">{{id}}</th>
        <td>{{login}}</td>
        <td>{{email}}</td>
        <td>{{image}}
          <img src="/{{image}}" width="50px" height="50px" alt={{login}} /></td>
      </tr>
    {{/each}}

  </tbody>
</table>